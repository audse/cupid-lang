package stdlib::stdlib

use [e] add [t: array [e]] with array [e] {
	fun [t] add = self, t other => {
		t mut copy = self
		for item in other => copy.push(item)
		copy
	}
}

use [r] fun [r] {
	fun [fun [r]] bind = self, int arg => {
		fun [r] bound = _ => {
			self(arg)
		}
		bound
	}
}

fun [int] square = int num => num ^ 2

fun [int] bound_square = square.bind(10)

log (bound_square())