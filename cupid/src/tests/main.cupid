package stdlib::stdlib

type parser = [
	string source,
	int index,
	int line,
]

use default [t: parser] with parser {
	fun [parser] default = _ => [
		source: '',
		index: 0,
		line: 0
	]
}

use parser {
	fun [parser] new = string source => [
		source: source,
		index: 0,
		line: 0
	]
	
	# fun [char] next = _ => {
	# 	self.index += 1
	# 	self.source.char(index)
	# }
}

parser mut cupid = parser.new('hello world')


log (cupid)