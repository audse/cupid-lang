class Foo {
    inFoo () {
        log ('in foo');
    }
}

class Bar < Foo {
    inBar () {
        log ('in bar');
    }
}

class Baz < Bar {
    inBaz () {
        log ('in baz');
    }
}

let baz = Baz();
baz.inFoo(); -- expect: in foo
baz.inBar(); -- expect: in bar
baz.inBaz(); -- expect: in baz
