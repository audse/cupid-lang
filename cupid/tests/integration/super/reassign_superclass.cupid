class Base {
    method () {
        log ('Base.method()');
    }
}

class Derived < Base {
    method () {
        super.method();
    }
}

class OtherBase {
    method () {
        log ('OtherBase.method()');
    }
}

let derived = Derived();
derived.method(); -- expect: Base.method()
Base = OtherBase;
derived.method(); -- expect: Base.method()
