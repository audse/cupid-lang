use int {
	
	fun [int] 
	abs = self => {
		if self > 0 => self
			else => -self
	}
	
	fun [int]
	clamp = self, int floor, int ceil => {
		if self > ceil => ceil
		else if self < floor => floor
		else => self
	}
	
	fun [bool]
	negative = self => self < 0
	
	fun [bool]
	positive = self => self > 0
	
	fun [int]
	sign = self => {
		if self > 1 => 1
			else if self < 0 => -1
			else => 0
	}
	
	fun [int] 
	sq = self => self ^ 2
}