
use equal! [t: type!] with type! {
	fun [t] equal! = self, t other => {
		bool name_match = self.name is other.name
		bool param_match = self.params is other.params
		bool member_match = self.members is other.members
		name_match and param_match and member_match
	}
}

use not_equal! [t: type!] with type! {
	fun [t] not_equal! = self, t other => {
		not (self is other)
	}
}
