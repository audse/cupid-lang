use [k, v] map [k, v] {

	fun [v]
	get = self, k key => get (self, key)

	fun [map [k, v]]
	set = mut self, k key, v value => set (self, key, value)
	
	fun [v]
	remove = mut self, k key => remove (self, key)
	
	fun [int]
	length = self => length (self)
	
	fun [bool]
	has_key = self, k key_name => {
		bool mut has_key = false
		for key, _ in self => {
			if key is key_name => has_key = true
		}
		has_key
	}
}